// First Line of Comment
Flow com.kudia.fido
     Basepath ""
     Timeout 100 seconds
     Configs configs
     Inputs userName passWord
// Assignements block
weddingDate = "01-12-2023"
location = "Jet Hotel"
time = "3PM"
// check wedding date
When weddingDate is 10
     // wedding description
     Log "@info start logging wedding event"
     // RFAC
     rfacCallResult = RFAC "https://www.kudia.com/check.html"
     it_tjwsm = { userId: "as9233Qz", property1: "value1"}
     Finish it_tjwsm
// Call Block
callResult | callException = Call Wedding#missing weddingDate
// rff block comment
rffResult = RRF "otp.ftl" time
// Description
Repeat 15 times max
     // Friday
     When weddingDate is "Friday"
          // tout
          Log "@info tout"
     // running
     Log "@info running"
// simple
Log "@info simple"